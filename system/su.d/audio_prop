#!/system/bin/sh 

setprop clock.allow_streaming_errors false
setprop root.max_latency 60
setprop ro.kernel.android.checkjni 0
setprop ro.kernel.checkjni 0
setprop ro.config.nocheckin 1
setprop audio.converter.samplerate.preferred libspeexresampler
setprop audio.converter.samplerate.libspeexresampler.quality best
setprop default.pcm.rate_converter speexrate_best
setprop af.resampler.quality 255
setprop persist.af.resampler.quality 255
setprop af.resample
setprop persist.af.resample
setprop audio.offload.buffer.size.kb 8184
setprop lpa.decode false
setprop lpa30.decode false
setprop lpa.use-stagefright false
setprop lpa.releaselock false
setprop lpa.deepbuffer.enable 0
setprop tunnel.decode false
setprop tunnel.audiovideo.decode false
setprop tunnel.audio.encode false
setprop tunnel.multiple false
setprop media.aac_51_output_enabled true
setprop use.non-omx.aac.decoder true
setprop media.stagefright.enable-aac true
setprop use.non-omx.mp3.decoder false
setprop audio.decoder_override_check true
setprop media.stagefright.enable-record false
setprop persist.audio.lowlatency.rec true
setprop ro.media.dec.aud.wma.enabled 1
setprop ro.config.music_lp_vol true
setprop ro.config.hw.music.lp true
setprop ro.ril.enable.amr.wideband 1
setprop audio.offload.buffer.size.kb 24
setprop audio.deep_buffer.media true
setprop audio.offload.gapless.enabled true
setprop persist.audio.vr.enable true
setprop mpq.audio.decode true
setprop persist.audio.hp true
setprop save_fread 0
setprop enable_aync_read 0
setprop enable_ftrace 0
setprop log_fps_interval 0
setprop dalvik.vm.heapgrowthlimit 384m
setprop dalvik.vm.heapsize 512m
setprop profiler.force_disable_err_rpt 1
setprop profiler.force_disable_ulog 1
setprop av.debug.disable.pers.cache true